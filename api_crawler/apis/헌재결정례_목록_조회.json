{
  "api_name": "헌재결정례 목록 조회",
  "request_url": "http://www.law.go.kr/DRF/lawSearch.do?target=detc",
  "request_parameters": [
    {
      "name": "OC",
      "type": "string",
      "required": true,
      "description": "사용자 이메일의 ID(g4c@korea.kr일경우 OC값=g4c)"
    },
    {
      "name": "target",
      "type": "string : detc",
      "required": true,
      "description": "서비스 대상"
    },
    {
      "name": "type",
      "type": "char",
      "required": true,
      "description": "출력 형태 : HTML/XML/JSON"
    },
    {
      "name": "search",
      "type": "int",
      "required": false,
      "description": "검색범위 (기본 : 1 헌재결정례명) 2 : 본문검색"
    },
    {
      "name": "query",
      "type": "string",
      "required": false,
      "description": "검색범위에서 검색을 원하는 질의 (정확한 검색을 위한 문자열 검색 query=\"자동차\")"
    },
    {
      "name": "display",
      "type": "int",
      "required": false,
      "description": "검색된 결과 개수 (default=20 max=100)"
    },
    {
      "name": "page",
      "type": "int",
      "required": false,
      "description": "검색 결과 페이지 (default=1)"
    },
    {
      "name": "gana",
      "type": "string",
      "required": false,
      "description": "사전식 검색(ga,na,da…,etc)"
    },
    {
      "name": "sort",
      "type": "string",
      "required": false,
      "description": "정렬옵션 (기본 : lasc 사건명 오름차순) ldes 사건명 내림차순 / dasc : 선고일자 오름차순 / ddes : 선고일자 내림차순 / nasc : 사건번호 오름차순 / ndes : 사건번호 내림차순 / efasc : 종국일자 오름차순 / efdes : 종국일자 내림차순"
    },
    {
      "name": "date",
      "type": "int",
      "required": false,
      "description": "종국일자"
    },
    {
      "name": "edYd",
      "type": "string",
      "required": false,
      "description": "종국일자 기간 검색"
    },
    {
      "name": "nb",
      "type": "int",
      "required": false,
      "description": "사건번호"
    },
    {
      "name": "popYn",
      "type": "string",
      "required": false,
      "description": "상세화면 팝업창 여부(팝업창으로 띄우고 싶을 때만 'popYn=Y')"
    }
  ],
  "response_fields": [
    {
      "name": "target",
      "type": "string",
      "description": "검색 대상"
    },
    {
      "name": "키워드",
      "type": "string",
      "description": "키워드"
    },
    {
      "name": "section",
      "type": "string",
      "description": "검색범위(EvtNm:헌재결정례명/bdyText:본문)"
    },
    {
      "name": "totalCnt",
      "type": "int",
      "description": "검색결과갯수"
    },
    {
      "name": "page",
      "type": "int",
      "description": "출력페이지"
    },
    {
      "name": "detc id",
      "type": "int",
      "description": "검색결과번호"
    },
    {
      "name": "헌재결정례일련번호",
      "type": "int",
      "description": "헌재결정례일련번호"
    },
    {
      "name": "종국일자",
      "type": "string",
      "description": "종국일자"
    },
    {
      "name": "사건번호",
      "type": "string",
      "description": "사건번호"
    },
    {
      "name": "사건명",
      "type": "string",
      "description": "사건명"
    },
    {
      "name": "헌재결정례상세링크",
      "type": "string",
      "description": "헌재결정례 상세링크"
    }
  ],
  "sample_urls": [
    "http://www.law.go.kr/DRF/lawSearch.do?OC=test&target=detc&type=XML&query=벌금",
    "http://www.law.go.kr/DRF/lawSearch.do?OC=test&target=detc&type=HTML&date=20150210",
    "http://www.law.go.kr/DRF/lawSearch.do?OC=test&target=detc&type=XML&query=자동차",
    "http://www.law.go.kr/DRF/lawSearch.do?OC=test&target=detc&type=JSON&query=자동차"
  ]
}

