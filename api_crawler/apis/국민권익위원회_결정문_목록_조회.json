{
  "api_name": "국민권익위원회 결정문 목록 조회",
  "request_url": "https://www.law.go.kr/DRF/lawSearch.do?target=acr",
  "request_parameters": [
    {
      "name": "OC",
      "type": "string",
      "required": true,
      "description": "사용자 이메일의 ID(g4c@korea.kr일경우 OC값=g4c)"
    },
    {
      "name": "target",
      "type": "string",
      "required": true,
      "description": "서비스 대상 (국민권익위원회 : acr)"
    },
    {
      "name": "type",
      "type": "char",
      "required": true,
      "description": "출력 형태 : HTML/XML/JSON"
    },
    {
      "name": "search",
      "type": "int",
      "required": false,
      "description": "검색범위 (1 : 민원표시 (default), 2 : 본문검색)"
    },
    {
      "name": "query",
      "type": "string",
      "required": false,
      "description": "검색범위에서 검색을 원하는 질의 (IE 조회시 UTF-8 인코딩 필수)"
    },
    {
      "name": "display",
      "type": "int",
      "required": false,
      "description": "검색된 결과 개수 (default=20 max=100)"
    },
    {
      "name": "page",
      "type": "int",
      "required": false,
      "description": "검색 결과 페이지 (default=1)"
    },
    {
      "name": "gana",
      "type": "string",
      "required": false,
      "description": "사전식 검색 (ga,na,da…,etc)"
    },
    {
      "name": "sort",
      "type": "string",
      "required": false,
      "description": "정렬옵션 (lasc : 민원표시 오름차순 (default) / ldes : 민원표시 내림차순 / dasc : 의결일 오름차순 / ddes : 의결일 내림차순 / nasc : 의안번호 오름차순 / ndes : 의안번호 내림차순)"
    },
    {
      "name": "popYn",
      "type": "string",
      "required": false,
      "description": "상세화면 팝업창 여부(팝업창으로 띄우고 싶을 때만 'popYn=Y')"
    }
  ],
  "response_fields": [
    {
      "name": "target",
      "type": "string",
      "description": "검색서비스 대상"
    },
    {
      "name": "키워드",
      "type": "string",
      "description": "검색 단어"
    },
    {
      "name": "section",
      "type": "string",
      "description": "검색범위"
    },
    {
      "name": "totalCnt",
      "type": "int",
      "description": "검색 건수"
    },
    {
      "name": "page",
      "type": "int",
      "description": "현재 페이지번호"
    },
    {
      "name": "기관명",
      "type": "string",
      "description": "기관명"
    },
    {
      "name": "acr id",
      "type": "int",
      "description": "검색 결과 순번"
    },
    {
      "name": "결정문일련번호",
      "type": "int",
      "description": "결정문 일련번호"
    },
    {
      "name": "제목",
      "type": "string",
      "description": "제목"
    },
    {
      "name": "민원표시명",
      "type": "string",
      "description": "민원표시명"
    },
    {
      "name": "의안번호",
      "type": "string",
      "description": "의안번호"
    },
    {
      "name": "회의종류",
      "type": "string",
      "description": "회의종류"
    },
    {
      "name": "결정구분",
      "type": "string",
      "description": "결정구분"
    },
    {
      "name": "의결일",
      "type": "string",
      "description": "의결일"
    },
    {
      "name": "결정문상세링크",
      "type": "string",
      "description": "결정문 상세링크"
    }
  ],
  "sample_urls": [
    "http://www.law.go.kr/DRF/lawSearch.do?OC=test&target=acr&type=HTML",
    "http://www.law.go.kr/DRF/lawSearch.do?OC=test&target=acr&type=XML",
    "http://www.law.go.kr/DRF/lawSearch.do?OC=test&target=acr&type=JSON"
  ]
}
