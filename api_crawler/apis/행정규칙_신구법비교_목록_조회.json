{
  "api_name": "행정규칙 신구법비교 목록 조회",
  "request_url": "http://www.law.go.kr/DRF/lawSearch.do?target=admrulOldAndNew",
  "note": "부가서비스는 행정규칙 서비스 신청을 하면 추가신청 없이 이용가능합니다.",
  "request_parameters": [
    {
      "name": "OC",
      "type": "string",
      "required": true,
      "description": "사용자 이메일의 ID(g4c@korea.kr일경우 OC값=g4c)"
    },
    {
      "name": "target",
      "type": "string : admrulOldAndNew",
      "required": true,
      "description": "서비스 대상"
    },
    {
      "name": "type",
      "type": "char",
      "required": true,
      "description": "출력 형태 HTML/XML/JSON"
    },
    {
      "name": "query",
      "type": "string",
      "required": false,
      "description": "법령명에서 검색을 원하는 질의 (정확한 검색을 위한 문자열 검색 query=\"자동차\")"
    },
    {
      "name": "display",
      "type": "int",
      "required": false,
      "description": "검색된 결과 개수 (default=20 max=100)"
    },
    {
      "name": "page",
      "type": "int",
      "required": false,
      "description": "검색 결과 페이지 (default=1)"
    },
    {
      "name": "org",
      "type": "string",
      "required": false,
      "description": "소관부처별 검색(소관부처코드 제공)"
    },
    {
      "name": "knd",
      "type": "string",
      "required": false,
      "description": "행정규칙 종류별 검색 (1=훈령/2=예규/3=고시/4=공고/5=지침/6=기타)"
    },
    {
      "name": "gana",
      "type": "string",
      "required": false,
      "description": "사전식 검색 (ga,na,da…,etc)"
    },
    {
      "name": "sort",
      "type": "string",
      "required": false,
      "description": "정렬옵션(기본 : lasc 법령오름차순) / ldes : 법령내림차순 / dasc : 발령일자 오름차순 / ddes : 발령일자 내림차순 / nasc : 발령번호 오름차순 / ndes : 발령번호 내림차순 / efasc : 시행일자 오름차순 / efdes : 시행일자 내림차순"
    },
    {
      "name": "date",
      "type": "string",
      "required": false,
      "description": "행정규칙 발령일자"
    },
    {
      "name": "prmlYd",
      "type": "string",
      "required": false,
      "description": "발령일자 기간검색(20090101~20090130)"
    },
    {
      "name": "nb",
      "type": "int",
      "required": false,
      "description": "행정규칙 발령번호 (예: 제2023-8호 검색을 원할시 nb=20238)"
    },
    {
      "name": "popYn",
      "type": "string",
      "required": false,
      "description": "상세화면 팝업창 여부(팝업창으로 띄우고 싶을 때만 'popYn=Y')"
    }
  ],
