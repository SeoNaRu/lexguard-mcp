{
  "api_name": "삭제 데이터 목록 조회",
  "request_url": "http://www.law.go.kr/DRF/lawSearch.do?target=delHst",
  "request_parameters": [
    {
      "name": "OC",
      "type": "string",
      "required": true,
      "description": "사용자 이메일의 ID(g4c@korea.kr일경우 OC값=g4c)"
    },
    {
      "name": "target",
      "type": "string",
      "required": true,
      "description": "서비스 대상 : datDel"
    },
    {
      "name": "type",
      "type": "string",
      "required": false,
      "description": "출력 형태 XML/JSON"
    },
    {
      "name": "display",
      "type": "int",
      "required": false,
      "description": "검색된 결과 개수 (default=20 max=100)"
    },
    {
      "name": "page",
      "type": "int",
      "required": false,
      "description": "검색 결과 페이지 (default=1)"
    },
    {
      "name": "knd",
      "type": "int",
      "required": false,
      "description": "데이터 종류 (법령 : 1 / 행정규칙 : 2 / 자치법규 : 3 / 학칙공단 : 13)"
    },
    {
      "name": "delDt",
      "type": "int",
      "required": false,
      "description": "데이터 삭제 일자 검색 (YYYYMMDD)"
    },
    {
      "name": "frmDt",
      "type": "int",
      "required": false,
      "description": "데이터 삭제 일자 범위 검색 시작일 (YYYYMMDD)"
    },
    {
      "name": "toDt",
      "type": "int",
      "required": false,
      "description": "데이터 삭제 일자 범위 검색 종료일 (YYYYMMDD)"
    }
  ],
  "response_fields": [
    {
      "name": "target",
      "type": "string",
      "description": "검색서비스 대상"
    },
    {
      "name": "totalCnt",
      "type": "int",
      "description": "검색건수"
    },
    {
      "name": "page",
      "type": "int",
      "description": "결과페이지번호"
    },
    {
      "name": "law id",
      "type": "int",
      "description": "결과 번호"
    },
    {
      "name": "일련번호",
      "type": "int",
      "description": "데이터 일련번호"
    },
    {
      "name": "구분명",
      "type": "string",
      "description": "데이터 구분명 (법령 / 행정규칙 / 자치법규 등)"
    },
    {
      "name": "삭제일자",
      "type": "string",
      "description": "데이터 삭제일자"
    }
  ],
  "sample_urls": [
    "http://www.law.go.kr/DRF/lawSearch.do?OC=test&target=delHst&type=XML",
    "http://www.law.go.kr/DRF/lawSearch.do?OC=test&target=delHst&knd=13&delDt=20231013&type=XML",
    "http://www.law.go.kr/DRF/lawSearch.do?OC=test&target=delHst&knd=3&frmDt=20231013&toDt=20231016&type=JSON"
  ]
}

