{
  "api_name": "영문법령 목록 조회",
  "request_url": "http://www.law.go.kr/DRF/lawSearch.do?target=elaw",
  "request_parameters": [
    {
      "name": "OC",
      "type": "string",
      "required": true,
      "description": "사용자 이메일의 ID(g4c@korea.kr일경우 OC값=g4c)"
    },
    {
      "name": "target",
      "type": "string : elaw",
      "required": true,
      "description": "서비스 대상"
    },
    {
      "name": "search",
      "type": "int",
      "required": false,
      "description": "검색범위 (기본 : 1 법령명) 2 : 본문검색"
    },
    {
      "name": "query",
      "type": "string",
      "required": false,
      "description": "법령명에서 검색을 원하는 질의(default=*)"
    },
    {
      "name": "display",
      "type": "int",
      "required": false,
      "description": "검색된 결과 개수 (default=20 max=100)"
    },
    {
      "name": "page",
      "type": "int",
      "required": false,
      "description": "검색 결과 페이지 (default=1)"
    },
    {
      "name": "sort",
      "type": "string",
      "required": false,
      "description": "정렬옵션(기본 : lasc 법령오름차순) / ldes : 법령내림차순 / dasc : 공포일자 오름차순 / ddes : 공포일자 내림차순 / nasc : 공포번호 오름차순 / ndes : 공포번호 내림차순 / efasc : 시행일자 오름차순 / efdes : 시행일자 내림차순"
    },
    {
      "name": "date",
      "type": "int",
      "required": false,
      "description": "법령의 공포일자 검색"
    },
    {
      "name": "efYd",
      "type": "string",
      "required": false,
      "description": "시행일자 범위 검색(20090101~20090130)"
    },
    {
      "name": "ancYd",
      "type": "string",
      "required": false,
      "description": "공포일자 범위 검색(20090101~20090130)"
    },
    {
      "name": "ancNo",
      "type": "string",
      "required": false,
      "description": "공포번호 범위 검색(306~400)"
    },
    {
      "name": "rrClsCd",
      "type": "string",
      "required": false,
      "description": "법령 제개정 종류 (300201-제정 / 300202-일부개정 / 300203-전부개정 / 300204-폐지 / 300205-폐지제정 / 300206-일괄개정 / 300207-일괄폐지 / 300209-타법개정 / 300210-타법폐지 / 300208-기타)"
    },
    {
      "name": "nb",
      "type": "int",
      "required": false,
      "description": "법령의 공포번호 검색"
    },
    {
      "name": "org",
      "type": "string",
      "required": false,
      "description": "소관부처별 검색(소관부처코드 제공)"
    },
    {
      "name": "knd",
      "type": "string",
      "required": false,
      "description": "법령종류(코드제공)"
    },
    {
      "name": "gana",
      "type": "string",
      "required": false,
      "description": "사전식 검색 (ga,na,da…,etc)"
    },
    {
      "name": "type",
      "type": "char",
      "required": false,
      "description": "출력 형태 HTML/XML/JSON"
    },
    {
      "name": "popYn",
      "type": "string",
      "required": false,
      "description": "상세화면 팝업창 여부(팝업창으로 띄우고 싶을 때만 'popYn=Y')"
    }
  ],
