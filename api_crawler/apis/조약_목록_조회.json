{
  "api_name": "조약 목록 조회",
  "request_url": "http://www.law.go.kr/DRF/lawSearch.do?target=trty",
  "request_parameters": [
    {
      "name": "OC",
      "type": "string",
      "required": true,
      "description": "사용자 이메일의 ID(g4c@korea.kr일경우 OC값=g4c)"
    },
    {
      "name": "target",
      "type": "string",
      "required": true,
      "description": "서비스 대상 (조약 : trty)"
    },
    {
      "name": "type",
      "type": "char",
      "required": true,
      "description": "출력 형태 : HTML/XML/JSON"
    },
    {
      "name": "search",
      "type": "int",
      "required": false,
      "description": "검색범위 (기본 : 1 조약명, 2 : 조약본문)"
    },
    {
      "name": "query",
      "type": "string",
      "required": false,
      "description": "검색범위에서 검색을 원하는 질의 (검색 결과 리스트)"
    },
    {
      "name": "display",
      "type": "int",
      "required": false,
      "description": "검색된 결과 개수 (default=20 max=100)"
    },
    {
      "name": "page",
      "type": "int",
      "required": false,
      "description": "검색 결과 페이지 (default=1)"
    },
    {
      "name": "gana",
      "type": "string",
      "required": false,
      "description": "사전식 검색 (ga,na,da…,etc)"
    },
    {
      "name": "eftYd",
      "type": "string",
      "required": false,
      "description": "발효일자 검색(20090101~20090130)"
    },
    {
      "name": "concYd",
      "type": "string",
      "required": false,
      "description": "체결일자 검색(20090101~20090130)"
    },
    {
      "name": "cls",
      "type": "int",
      "required": false,
      "description": "1 : 양자조약, 2 : 다자조약"
    },
    {
      "name": "natCd",
      "type": "int",
      "required": false,
      "description": "국가코드"
    },
    {
      "name": "sort",
      "type": "string",
      "required": false,
      "description": "정렬옵션 (기본 : lasc 조약명오름차순) / ldes : 조약명내림차순 / dasc : 발효일자 오름차순 / ddes : 발효일자 내림차순 / nasc : 조약번호 오름차순 / ndes : 조약번호 내림차순 / rasc : 관보게재일 오름차순 / rdes : 관보게재일 내림차순"
    },
    {
      "name": "popYn",
      "type": "string",
      "required": false,
      "description": "상세화면 팝업창 여부(팝업창으로 띄우고 싶을 때만 'popYn=Y')"
    }
  ],
