{
  "api_name": "자치법규 별표서식 목록 조회",
  "request_url": "http://www.law.go.kr/DRF/lawSearch.do?target=ordinbyl",
  "request_parameters": [
    {
      "name": "OC",
      "type": "string",
      "required": true,
      "description": "사용자 이메일의 ID(g4c@korea.kr일경우 OC값=g4c)"
    },
    {
      "name": "target",
      "type": "string",
      "required": true,
      "description": "서비스 대상 (자치법규 별표서식 : ordinbyl)"
    },
    {
      "name": "type",
      "type": "char",
      "required": true,
      "description": "출력 형태 HTML/XML/JSON"
    },
    {
      "name": "search",
      "type": "int",
      "required": false,
      "description": "검색범위 (기본 : 1 별표서식명, 2 : 해당자치법규명검색, 3 : 별표본문검색)"
    },
    {
      "name": "query",
      "type": "string",
      "required": false,
      "description": "법령명에서 검색을 원하는 질의(default=*) (정확한 검색을 위한 문자열 검색 query=\"자동차\")"
    },
    {
      "name": "display",
      "type": "int",
      "required": false,
      "description": "검색된 결과 개수 (default=20 max=100)"
    },
    {
      "name": "page",
      "type": "int",
      "required": false,
      "description": "검색 결과 페이지 (default=1)"
    },
    {
      "name": "sort",
      "type": "string",
      "required": false,
      "description": "정렬옵션 (기본 : lasc 별표서식명 오름차순) / ldes : 별표서식명 내림차순"
    },
    {
      "name": "org",
      "type": "string",
      "required": false,
      "description": "소관부처별 검색(소관부처코드 제공)"
    },
    {
      "name": "sborg",
      "type": "string",
      "required": false,
      "description": "지자체별 시·군·구 검색(지자체코드 제공) (필수값 : org, ex.서울특별시 구로구에 대한 검색-> org=6110000&sborg=3160000)"
    },
    {
      "name": "knd",
      "type": "string",
      "required": false,
      "description": "별표종류 (1 : 별표, 2 : 서식, 3 : 별도, 4 : 별지)"
    },
    {
      "name": "gana",
      "type": "string",
      "required": false,
      "description": "사전식 검색(ga,na,da…,etc)"
    },
    {
      "name": "popYn",
      "type": "string",
      "required": false,
      "description": "상세화면 팝업창 여부(팝업창으로 띄우고 싶을 때만 'popYn=Y')"
    }
  ],
  "response_fields": [
    {
      "name": "target",
      "type": "string",
      "description": "검색서비스 대상"
    },
    {
      "name": "키워드",
      "type": "string",
      "description": "검색 단어"
    },
    {
      "name": "section",
      "type": "string",
      "description": "검색범위"
    },
    {
      "name": "totalCnt",
      "type": "int",
      "description": "검색건수"
    },
    {
      "name": "page",
      "type": "int",
      "description": "현재 페이지번호"
    },
    {
      "name": "ordinbyl id",
      "type": "int",
      "description": "검색 결과 순번"
    },
    {
