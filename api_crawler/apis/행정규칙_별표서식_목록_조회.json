{
  "api_name": "행정규칙 별표서식 목록 조회",
  "request_url": "http://www.law.go.kr/DRF/lawSearch.do?target=admbyl",
  "request_parameters": [
    {
      "name": "OC",
      "type": "string",
      "required": true,
      "description": "사용자 이메일의 ID(g4c@korea.kr일경우 OC값=g4c)"
    },
    {
      "name": "target",
      "type": "string",
      "required": true,
      "description": "서비스 대상 (행정규칙 별표서식 : admbyl)"
    },
    {
      "name": "type",
      "type": "char",
      "required": true,
      "description": "출력 형태 HTML/XML/JSON"
    },
    {
      "name": "search",
      "type": "int",
      "required": false,
      "description": "검색범위 (기본 : 1 별표서식명, 2 : 해당법령검색, 3 : 별표본문검색)"
    },
    {
      "name": "query",
      "type": "string",
      "required": false,
      "description": "법령명에서 검색을 원하는 질의(default=*) (정확한 검색을 위한 문자열 검색 query=\"자동차\")"
    },
    {
      "name": "display",
      "type": "int",
      "required": false,
      "description": "검색된 결과 개수 (default=20 max=100)"
    },
    {
      "name": "page",
      "type": "int",
      "required": false,
      "description": "검색 결과 페이지 (default=1)"
    },
    {
      "name": "sort",
      "type": "string",
      "required": false,
      "description": "정렬옵션 (기본 : lasc 별표서식명 오름차순) / ldes : 별표서식명 내림차순"
    },
    {
      "name": "org",
      "type": "string",
      "required": false,
      "description": "소관부처별 검색(소관부처코드 제공)"
    },
    {
      "name": "knd",
      "type": "string",
      "required": false,
      "description": "별표종류 (1 : 별표, 2 : 서식, 3 : 별지)"
    },
    {
      "name": "gana",
      "type": "string",
      "required": false,
      "description": "사전식 검색(ga,na,da…,etc)"
    },
    {
      "name": "popYn",
      "type": "string",
      "required": false,
      "description": "상세화면 팝업창 여부(팝업창으로 띄우고 싶을 때만 'popYn=Y')"
    }
  ],
  "response_fields": [
